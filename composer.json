{
    "name": "wnull/warface-api",
    "description": "Convenient library for working with the Warface API on PHP",
    "keywords": ["warface", "api", "client"],
    "type": "library",
    "homepage": "https://github.com/wnull/warface-api",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-dom": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4.3"
    },
    "autoload": {
        "psr-4": {
            "Warface\\": "src/warface-api"
        }
    }
}
