{
    "name": "wnull/warface-api",
    "description": "A fast, convenient and easy library for working with the Warface API written in PHP.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-json": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.1",
        "doctrine/coding-standard": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Warface\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WarfaceTest\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
